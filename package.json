{
  "name": "rempl",
  "version": "0.1.1",
  "description": "Rempl host for Atom Editor",
  "main": "lib/index.js",
  "repository": "rempl/host-atom",
  "engines": {
    "atom": ">=1.7.0 <2.0.0"
  },
  "keywords": [
    "atom",
    "rempl"
  ],
  "author": {
    "name": "Sergey Melyukov",
    "email": "s.melukov@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rempl/host-atom/issues"
  },
  "homepage": "https://github.com/rempl/host-atom",
  "eslintConfig": {
    "env": {
      "modules": true,
      "node": true,
      "mocha": true,
      "es6": true
    },
    "rules": {
      "no-duplicate-case": "error",
      "no-undef": "error",
      "no-unused-vars": [
        "error",
        {
          "vars": "all",
          "args": "after-used"
        }
      ]
    }
  },
  "scripts": {
    "codestyle": "jscs ./lib && eslint ./src"
  },
  "devDependencies": {
    "eslint": "^3.11.1",
    "jscs": "^3.0.7"
  }
}
